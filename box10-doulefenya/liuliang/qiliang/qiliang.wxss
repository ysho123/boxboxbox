/* pages/qiliang/qiliang.wxss */
page{
  width: 100%;
  height: 100%;
  background: #eee;
  overflow: hidden;
}
.qiliang_con{
  width: 700rpx;
  background: #fff;
  border-radius: 15rpx;
  margin: 30rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
.qiliang_img{
  width: 650rpx;
  border-radius: 15rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}
.start_btn{
  width: 650rpx;
  height: 100rpx;
  background: #54A3C3;
  border: none;
  border-radius: 10rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 40rpx;
  line-height: 100rpx;
}

.qiliang_form{
  position: relative;
}
.formBtn{
  position: absolute;
  left: 0;
  top: 0;
  width: 650rpx;
  height: 100rpx;
}
button[plain]{
  border: none;
}