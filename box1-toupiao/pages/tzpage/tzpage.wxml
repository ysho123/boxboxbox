<!--pages/others/others.wxml-->
<view class="others">
  <view class="others_item" bindtap='toXcx' wx:for="{{xcxlist}}" wx:key="xcxlist" data-appid="{{item.appid}}">
    <image class="xcx_pic" src="{{item.image}}"></image>
    <view class="item_mid">
      <view class="xcx_title">
        <text>{{item.name}}</text>
        <image class="icon_star" wx:for="{{5}}" wx:key="img" src="../../images/other_star.png"></image>
      </view>
      <view class="intro">{{item.intro}}</view>
    </view>
    <image class='icon_rtarr' src='../../images/other_arrow.png'></image>  
  </view>
</view>
