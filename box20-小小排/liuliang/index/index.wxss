/* 2018.07.9改版廖敏芝 */
.index{
  padding: 54rpx 0;
}
.index_container{
  position: relative;
  top: 0;
  left: 0;
  margin-bottom: 64rpx;
}
.list_item{
  width:680rpx;
  height: 636rpx;
  border-radius: 30rpx;
  box-shadow:0 0 30rpx rgba(0,0,0,0.3);
  background: #fff;
  margin: 0 auto;
}

.formBtn {
    width:680rpx;
    height:636rpx;
    position: absolute;
    bottom: 0;
    left:50%;
    transform: translate(-50%, 0);
    z-index: 100;
}

.formBtn[plain] {
    border: 0;
}
.list_bg{
  position: relative;
  top: 0;
  left: 0;
  width: 680rpx;
  height: 500rpx;
}
.banner{
  width: 680rpx;
  height: 500rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
}
.bg_title{
  position: absolute;
  bottom: 30rpx;
  left: 32rpx;
  font-size: 50rpx;
  color:#fff;
  font-weight: bold;
  width: 680rpx;
  overflow: hidden;
  text-overflow: ellipsis;
    white-space: nowrap;
    -webkit-white-space: nowrap;
}
.list_info{
  height: 136rpx;
  display: -webkit-box;
  -webkit-box-orient: horizontal;
  -webkit-box-align:center;
}
.logo{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-left: 32rpx;
}
.list_title{
  margin-left: 22rpx;
  -webkit-box-flex: 1;
}
.list_btn{
  width: 160rpx;
  height: 60rpx;
  background: #00b7fe;
  border-radius: 30rpx;
  color: #fff;
  text-align: center;
  margin-right: 44rpx;
  font-size: 30rpx;
  line-height: 60rpx;
}
.navigator{
  position: absolute;
  top: 0;
  left:50%;
  transform: translate(-50%, 0);
  width:680rpx;
  height: 636rpx;
  z-index: 100;


}