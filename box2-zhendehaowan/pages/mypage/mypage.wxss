/* pages/mypage/mypage.wxss */
page{
   background:#f5f5f5;
}
.top{
   overflow:hidden;
   background:#fff;
   position:fixed;
   top:0;
   left:0;
   z-index:500;
   width:100%;
}

.topLeft{
   float:left;
   width:50%;
   height:98rpx;
   line-height:92rpx;
   font-size:30rpx;
   text-align:center;
   box-sizing:border-box;
}
.topRight{
   float:left;
   width:50%;
   height:98rpx;
   line-height:92rpx;
   font-size:30rpx;
   color:#999;
   text-align:center;
   box-sizing:border-box;
}

.content{
   margin-top:98rpx;
   overflow:hidden;
}
.content_c{
   width:690rpx;
   height:160rpx;
   background:#fff;
   border-radius:4rpx;
   margin:0 auto;
   margin-top:30rpx;
   position:relative;
}
.content_img{
   width:122rpx;
   height:122rpx;
   vertical-align: top;
   margin:19rpx 20rpx 0 20rpx;
   border:1px solid #e5e5e5;
   box-sizing: border-box; 
   border-radius: 10rpx;
   float:left;
}
.content_text{
   float:left;
}
.content_title{
   width:450rpx;
   font-size: 32rpx;
   color: #000;
   margin-top:14rpx;
   overflow:hidden;
   text-overflow:ellipsis;
   white-space:nowrap;
   -webkit-white-space:nowrap;
}
.content_num{
   font-size: 26rpx;
   color: #999;
   margin-top: 7rpx;
}
.content_statu{
   font-size: 26rpx;
   color: #fecb00;
   margin-top: 7rpx;
}
.content_statu2{
   font-size: 26rpx;
   color: #999;
   margin-top: 7rpx;
}
.delete{
   width:70rpx;
   height:70rpx;
   text-align: center;
   position:absolute;
   top:45rpx;
   right:20rpx;
   z-index:10;
   
}
.delete_img{
   width:30rpx;
   height:30rpx;
   margin-top:20rpx;
}
.content_bo{
   overflow:hidden;
}

.null_data{
   margin-top:337rpx;
   text-align: center;
}
.null_img{
   width:142rpx;
   height:121rpx;
   vertical-align: bottom;
}
.null_txt{
   font-size: 28rpx;
   color:#b2b2b2;
   margin-top: 20rpx;
}



